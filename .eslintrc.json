{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": ["plugin:react/recommended", "standard"],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"indent": [1, "tab"],
		"jsx-quotes": [1, "prefer-single"],
		"quotes": [1, "single"],
		"no-tabs": 0,
		"react/react-in-jsx-scope": "off",
		"react/prop-types": "off",
		"no-unused-vars": 1
	}
}
